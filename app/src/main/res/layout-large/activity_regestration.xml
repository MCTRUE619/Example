<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="20dp"
            android:layout_height="16dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="16.38dp"
            android:layout_marginTop="19.78dp"
            android:clickable="true"
            android:onClick="BackReg"
            app:srcCompat="@drawable/arrow2"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.5"
            app:srcCompat="@drawable/aspire_logo"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />
    </RelativeLayout>
    <com.google.android.flexbox.FlexboxLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:alignContent="center"
        app:alignItems="center"
        app:flexDirection="column"
        app:justifyContent="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="123dp"
            android:fontFamily="sans-serif-medium"
            android:gravity="center_horizontal"
            android:letterSpacing="-0.041"
            android:lineHeight="22px"
            android:text="Создать профиль"
            android:textAlignment="center"
            android:textColor="@color/slider"
            android:textFontWeight="600"
            android:textSize="22dp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/name"
            android:layout_width="327dp"
            android:layout_height="53dp"
            android:imeOptions="actionDone"
            android:layout_marginTop="69dp"
            android:background="@drawable/rectangle"
            android:ems="10"
            android:inputType="textPersonName"
            android:letterSpacing="0.05"
            android:paddingLeft="30dp"
            android:text=""
            android:hint="Имя в программе"
            android:textColor="#3E333F"
            android:textFontWeight="400"
            android:textSize="14dp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />
        <EditText
            android:id="@+id/phone"
            android:layout_width="327dp"
            android:layout_height="53dp"
            android:imeOptions="actionDone"
            android:layout_marginTop="19dp"
            android:background="@drawable/rectangle"
            android:ems="10"
            android:inputType="phone"
            android:letterSpacing="0.05"
            android:maxLength="10"
            android:paddingLeft="30dp"
            android:text=""
            android:textColor="#3E333F"
            android:textFontWeight="400"
            android:textSize="14dp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />
        <TextView
            android:id="@+id/phonetext"
            android:layout_width="wrap_content"
            android:layout_height="11dp"
            android:layout_marginLeft="-230dp"
            android:layout_marginTop="-58dp"
            android:background="#FFFFFF"
            android:paddingLeft="7dp"
            android:text="Телефон "
            android:textColor="@color/slider2"
            android:textFontWeight="400"
            android:textSize="10dp"
            android:textStyle="normal"
            android:fontFamily="sans-serif"
            app:layout_constraintStart_toStartOf="@+id/phone"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />
        <TextView
            android:id="@+id/nametext"
            android:layout_width="44dp"
            android:layout_height="11dp"
            android:layout_marginLeft="-230dp"
            android:layout_marginTop="-85dp"
            android:background="#FFFFFF"
            android:paddingLeft="7dp"
            android:text="Логин"
            android:textColor="@color/slider2"
            android:textFontWeight="400"
            android:textSize="10dp"
            android:textStyle="normal"
            android:fontFamily="sans-serif"
            app:layout_constraintStart_toStartOf="@+id/name"
            app:layout_constraintTop_toBottomOf="@+id/name" />



        <EditText
            android:id="@+id/Passwordvvod"
            android:layout_width="327dp"
            android:layout_height="53dp"
            android:imeOptions="actionDone"
            android:layout_marginTop="136dp"
            android:background="@drawable/rectangle"
            android:ems="10"
            android:inputType="textPassword"
            android:hint="Пароль от 8 символов"

            android:paddingLeft="30dp"
            android:fontFamily="sans-serif"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/phone"
            android:autofillHints="" />




        <EditText
            android:id="@+id/Passwordvvod2"
            android:layout_width="327dp"
            android:layout_height="53dp"
            android:imeOptions="actionDone"
            android:layout_marginTop="25dp"
            android:background="@drawable/rectangle"
            android:ems="10"
            android:inputType="textPassword"
            android:hint="Повторите пароль"
            android:paddingLeft="30dp"
            android:text=""
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2" />
        <Button
            android:id="@+id/button2"
            android:clickable="true"
            android:onClick="openMain"
            android:layout_width="314dp"
            android:layout_height="55dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="31dp"
            android:background="@drawable/button_rec"
            android:text="Войти в свой аккаунт"
            android:textAlignment="center"
            android:fontFamily="sans-serif"
            android:textColor="@color/white"
            android:textFontWeight="500"
            android:textSize="14dp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword" />
        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="72dp"
            app:layout_constraintHorizontal_bias="0.5"
            app:srcCompat="@drawable/aspire_logo"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

    </com.google.android.flexbox.FlexboxLayout>

</androidx.constraintlayout.widget.ConstraintLayout>