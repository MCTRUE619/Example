<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="72dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="20dp"
            android:layout_height="16dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="16.38dp"
            android:layout_marginTop="19.78dp"
            android:clickable="true"
            android:onClick="BackReg"
            app:srcCompat="@drawable/arrow2"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.5"
            app:srcCompat="@drawable/aspire_logo"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />
    </RelativeLayout>
<com.google.android.flexbox.FlexboxLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:alignContent="center"
    app:alignItems="center"
    app:flexDirection="column"
    app:justifyContent="center"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="133dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal"
        android:letterSpacing="-0.041"
        android:lineHeight="22px"
        android:text="Создать профиль"
        android:textAlignment="center"
        android:textColor="@color/slider"
        android:textFontWeight="600"
        android:textSize="22dp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="385dp"
        android:layout_height="60dp"
        android:imeOptions="actionDone"
        android:layout_marginTop="39dp"
        android:background="@drawable/rectangle"
        android:ems="10"
        android:inputType="textPersonName"
        android:letterSpacing="0.05"
        android:paddingLeft="30dp"
        android:text=""
        android:textColor="#3E333F"
        android:textFontWeight="400"
        android:textSize="12dp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />


    <TextView
        android:id="@+id/textView8"
        android:layout_width="44dp"
        android:layout_height="12dp"
        android:layout_marginLeft="-250dp"
        android:layout_marginTop="-65dp"
        android:background="#FFFFFF"
        android:paddingLeft="7dp"
        android:text="Логин"
        android:textColor="@color/slider2"
        android:textFontWeight="400"
        android:textSize="10dp"
        android:textStyle="normal"
        android:fontFamily="sans-serif"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName2"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />



    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="385dp"
        android:layout_height="60dp"
        android:imeOptions="actionDone"
        android:layout_marginTop="75dp"
        android:background="@drawable/rectangle"
        android:ems="10"
        android:inputType="textPassword"
        android:hint="Пароль"
        android:paddingLeft="30dp"
        android:fontFamily="sans-serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
        android:autofillHints="" />




    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="385dp"
        android:layout_height="60dp"
        android:imeOptions="actionDone"
        android:layout_marginTop="20dp"
        android:background="@drawable/rectangle"
        android:ems="10"
        android:inputType="textPassword"
        android:hint="Пароль"
        android:paddingLeft="30dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2" />
    <Button
        android:id="@+id/button2"
        android:clickable="true"
        android:onClick="openMain"
        android:layout_width="385dp"
        android:layout_height="60dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="60dp"
        android:background="@drawable/button_rec"
        android:text="Войти в свой аккаунт"
        android:textAlignment="center"
        android:fontFamily="sans-serif"
        android:textColor="@color/white"
        android:textFontWeight="500"
        android:textSize="14dp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-35dp"
        android:layout_marginTop="50dp"
        android:layout_marginRight="230dp"
        android:text="Войти через соц сети"
        android:textColor="@color/slider2"
        android:textFontWeight="500"
        android:textSize="15dp"
        android:fontFamily="sans-serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.178"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />
    <com.google.android.flexbox.FlexboxLayout
        android:id="@+id/flexi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        app:alignContent="space_around"
        app:alignItems="center"
        app:justifyContent="space_around"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">


        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/imageView"
            android:layout_width="110dp"
            android:layout_height="65dp"
            android:clickable="true"
            android:onClick="myFun1"
            android:background="@drawable/shadow"
            app:srcCompat="@drawable/telegram_icon" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/imageView3"
            android:layout_width="110dp"
            android:layout_height="65dp"
            android:clickable="true"
            android:onClick="myFun2"
            android:background="@drawable/shadow"
            app:srcCompat="@drawable/viber_icon" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/imageView2"
            android:layout_width="110dp"
            android:layout_height="65dp"
            android:clickable="true"
            android:onClick="myFun3"
            android:background="@drawable/shadow"
            app:srcCompat="@drawable/google_icon" />
    </com.google.android.flexbox.FlexboxLayout>
</com.google.android.flexbox.FlexboxLayout>
</androidx.constraintlayout.widget.ConstraintLayout>